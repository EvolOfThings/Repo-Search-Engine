{
    "parser": "babel-eslint",
    "plugins": ["react"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true,
            "jsx": true
        }
    },
    "env": {
        "es6":     true,
        "browser": true,
        "node":    true,
        "mocha":   true
    },
    "globals": {
      "nconf": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],


    "rules": {
    }
}
